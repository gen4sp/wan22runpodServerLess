{
    "name": "wan22-i2v-worker",
    "description": "WAN 2.2 Image-to-Video Generation Worker optimized for RTX 5090",
    "builder": "base",
    "baseImage": "runpod/pytorch:2.8.0-py3.11-cuda12.8.1-cudnn-devel-ubuntu22.04",
    "repository": "https://github.com/gen4sp/wan22runpodServerLess",
    "branch": "main",
    "gpuTypeId": "NVIDIA_RTX_4090",
    "containerDiskInGb": 50,
    "volumeMounts": [
        {
            "name": "comfy-models-volume",
            "mountPath": "/runpod-volume"
        }
    ],
    "env": [
        {
            "key": "TORCH_COMPILE",
            "value": "1"
        },
        {
            "key": "TORCH_CUDA_ARCH_LIST",
            "value": "9.0"
        },
        {
            "key": "PYTORCH_CUDA_ALLOC_CONF",
            "value": "max_split_size_mb:512"
        }
    ],
    "ports": "8188/http",
    "scalerType": "QUEUE_DELAY",
    "scalerValue": 4,
    "workersMin": 0,
    "workersMax": 3,
    "idleTimeout": 5,
    "flashBoot": false
}
